<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UserServiceTestSuit" parallel="true">

	<test name="getCurrentUserProgramListTest()" preserve-order="false">
		<classes>
			<class name="com.erecyclingcorps.service.UserServiceTest">
				<methods>
					<include name="getCurrentUserProgramListTest" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="getUserProgramDataOnLoadTest()" preserve-order="false">
		<parameter name="program" value="ATT" />
		<classes>
			<class name="com.erecyclingcorps.service.UserServiceTest">
				<methods>
					<include name="getUserProgramDataOnLoadTest" />
				</methods>
			</class>
		</classes>
	</test>

</suite>